{
  "name": "@akrdevtech/lib-express-request-id",
  "version": "1.0.2",
  "description": "Request/ Transaction id generator for Express Js",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:cjs": "tsc --build tsconfig.json",
    "build:esm": "tsc --build tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts",
    "version:up": "npm version patch && npm install && npm run build",
    "publish": "npm publish",
    "publish:up": "npm run version:up && npm run publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akrdevtech/lib-express-request-id.git"
  },
  "keywords": [
    "express",
    "middleware",
    "request id",
    "uuid",
    "transaction id"
  ],
  "author": "Akhil Krishnan R",
  "license": "ISC",
  "files": [
    "dist/**/*"
  ],
  "bugs": {
    "url": "https://github.com/akrdevtech/lib-express-request-id/issues"
  },
  "homepage": "https://github.com/akrdevtech/lib-express-request-id#readme",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "express": "^5.1.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
}
